/*Створіть програму, яка обчислює факторіал натурального числа n, яке користувач
введе з клавіатури.*/
import java.util.Scanner;
public class task8 {
  public static void main(String[] args) {
           int a;
        System.out.println("Введіть число");
        Scanner sc = new Scanner(System.in);
        if (sc.hasNextInt()) {
            a = sc.nextInt();
            int y = 1;
            for (int i = 1; i <= a; i++) {
                y = y * i;
            }
            System.out.print(a + "!=");
            System.out.print(y);
        }
    }
} 
